    "textlint-rule-preset-ja-spacing": "^2.0.2"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run linter:js"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write",
      "npm run linter:css -- --fix"
    ],
    "*.md": [
      "npm run linter:textfix"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "linter:js": "eslint --ext .js,.jsx --ignore-pattern public src",
    "linter:css": "node_modules/.bin/stylelint \"src/**/*.scss\"",
    "linter:text": "textlint \"src/pages/**/index.md\"",
    "linter:textfix": "textlint --fix \"src/pages/**/index.md\"",
    "test": "jest",
    "e2e": "cypress run"
  },
  "peerDependencies": {
    "prismjs": "^1.13.0",
    "react": "^15.6.2",
    "react-dom": "15.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
