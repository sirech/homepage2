    "lighthouse": "^5.6.0",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "stylelint": "^13.3.3",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
