{
  "name": "homepage",
  "description": "My personal website",
  "version": "0.2.0",
  "author": "Mario Fernandez",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.0.12",
    "gatsby-paginate": "^1.0.17",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-catch-links": "^2.0.3",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-feed": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-layout": "^1.0.2",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-twitter": "^2.0.5",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-prismjs": "^3.0.1",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.4",
    "gatsby-transformer-sharp": "^2.1.3",
    "node-sass": "^4.9.3",
    "prismjs": "^1.13.0",
    "ramda": "^0.26.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "bootstrap": "^4.1.1",
    "coveralls": "^3.0.0",
    "devicon-2.2": "^2.2.0",
    "emergence.js": "^1.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^4.0.0",
    "font-awesome": "^4.7.0",
    "husky": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.14.0",
    "react-test-renderer": "^16.5.2",
    "regenerator-runtime": "^0.13.1",
    "stylelint": "^9.2.1",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-scss": "^3.1.3",
    "textlint": "^11.0.0",
    "textlint-rule-preset-ja-spacing": "^2.0.1"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run linter:js",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "npm run linter:css -- --fix",
      "git add"
    ],
    "*.md": [
      "npm run linter:textfix",
      "git add"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "linter:js": "eslint --ext .js,.jsx --ignore-pattern public src",
    "linter:css": "node_modules/.bin/stylelint \"src/**/*.scss\"",
    "linter:text": "textlint \"src/pages/**/index.md\"",
    "linter:textfix": "textlint --fix \"src/pages/**/index.md\"",
    "test": "jest"
  },
  "peerDependencies": {
    "prismjs": "^1.13.0",
    "react": "^15.6.2",
    "react-dom": "15.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
