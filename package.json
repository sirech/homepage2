{
  "name": "homepage",
  "description": "My personal website",
  "version": "0.2.0",
  "author": "Mario Fernandez",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.14.0",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-catch-links": "^2.1.5",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-feed": "^2.3.9",
    "gatsby-plugin-layout": "^1.1.5",
    "gatsby-plugin-nprogress": "^2.1.4",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.2.0",
    "gatsby-plugin-sass": "^2.1.12",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-plugin-sitemap": "^2.2.9",
    "gatsby-plugin-twitter": "^2.1.4",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.3",
    "gatsby-remark-copy-linked-files": "^2.1.13",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.19",
    "gatsby-remark-prismjs": "^3.3.9",
    "gatsby-remark-responsive-iframe": "^2.2.10",
    "gatsby-remark-smartypants": "^2.1.5",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-remark": "^2.6.19",
    "gatsby-transformer-sharp": "^2.2.12",
    "node-sass": "^4.9.3",
    "prismjs": "^1.17.1",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.6.2"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.11",
    "bootstrap": "^4.1.1",
    "coveralls": "^3.0.6",
    "cypress": "^3.4.1",
    "devicon-2.2": "^2.2.0",
    "emergence.js": "^1.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "font-awesome": "^4.7.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.4",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "regenerator-runtime": "^0.13.3",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-scss": "^3.9.4",
    "textlint": "^11.3.1",
    "textlint-rule-preset-ja-spacing": "^2.0.1"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run linter:js",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "npm run linter:css -- --fix",
      "git add"
    ],
    "*.md": [
      "npm run linter:textfix",
      "git add"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "linter:js": "eslint --ext .js,.jsx --ignore-pattern public src",
    "linter:css": "node_modules/.bin/stylelint \"src/**/*.scss\"",
    "linter:text": "textlint \"src/pages/**/index.md\"",
    "linter:textfix": "textlint --fix \"src/pages/**/index.md\"",
    "test": "jest",
    "e2e": "cypress run"
  },
  "peerDependencies": {
    "prismjs": "^1.13.0",
    "react": "^15.6.2",
    "react-dom": "15.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
