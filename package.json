{
  "name": "homepage",
  "description": "My personal website",
  "version": "0.2.0",
  "author": "Mario Fernandez",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.7.1",
    "gatsby-paginate": "^1.0.17",
    "gatsby-plugin-canonical-urls": "^2.0.13",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-feed": "^2.2.1",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-layout": "^1.0.13",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-sharp": "^2.1.1",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-twitter": "^2.0.13",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.12",
    "gatsby-remark-prismjs": "^3.2.5",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-remark": "^2.3.4",
    "gatsby-transformer-sharp": "^2.1.17",
    "node-sass": "^4.9.3",
    "prismjs": "^1.16.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0"
  },
  "devDependencies": {
    "babel-jest": "^24.7.1",
    "babel-preset-gatsby": "^0.1.11",
    "bootstrap": "^4.1.1",
    "coveralls": "^3.0.0",
    "cypress": "^3.2.0",
    "devicon-2.2": "^2.2.0",
    "emergence.js": "^1.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^4.0.0",
    "font-awesome": "^4.7.0",
    "husky": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-scss": "^3.6.0",
    "textlint": "^11.0.0",
    "textlint-rule-preset-ja-spacing": "^2.0.1"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run linter:js",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "npm run linter:css -- --fix",
      "git add"
    ],
    "*.md": [
      "npm run linter:textfix",
      "git add"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|scss)\"",
    "linter:js": "eslint --ext .js,.jsx --ignore-pattern public src",
    "linter:css": "node_modules/.bin/stylelint \"src/**/*.scss\"",
    "linter:text": "textlint \"src/pages/**/index.md\"",
    "linter:textfix": "textlint --fix \"src/pages/**/index.md\"",
    "test": "jest",
    "e2e": "cypress run"
  },
  "peerDependencies": {
    "prismjs": "^1.13.0",
    "react": "^15.6.2",
    "react-dom": "15.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
